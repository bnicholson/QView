# Get all the dependencies we need for this application

## first make sure we are in the frontend dir
cd frontend

# now get the deps
yarn add react
yarn add react-dom
yarn add @babel/core
yarn add @mui/material @emotion/react @emotion/styled
yarn add @mui/icons-material
yarn add @reduxjs/toolkit react-redux
yarn add @mui/styles
#
# Datepicker
#
yarn add dayjs
yarn add '@mui/x-date-pickers'
yarn add '@mui/system'
#
# Virtualized Table
yarn add react-virtualized
